# Nebuchadnezzar - Biological Quantum Computer Framework
# Environment Configuration

# Database Configuration
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=nebuchadnezzar
POSTGRES_USER=nebuchadnezzar

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# MinIO Object Storage
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin123
MINIO_HOST=localhost
MINIO_PORT=9000

# Monitoring
GRAFANA_PASSWORD=admin123
PROMETHEUS_HOST=localhost
PROMETHEUS_PORT=9090

# Application Configuration
RUST_LOG=info
RUST_BACKTRACE=1
NEBUCHADNEZZAR_PORT=8080
NEBUCHADNEZZAR_HOST=0.0.0.0

# Scientific Computing Parameters
ATP_POOL_SIZE=1000.0
OSCILLATION_FREQUENCY=1.0
MEMBRANE_POTENTIAL=-70.0
TEMPERATURE=310.15
PH=7.4

# Quantum Computation Settings
COHERENCE_TIME=1e-12
DECOHERENCE_RATE=1e9
TUNNELING_BARRIER_HEIGHT=0.5
QUANTUM_STATE_COUNT=8

# Simulation Parameters
MAX_INTEGRATION_STEPS=10000
CONVERGENCE_THRESHOLD=1e-6
ADAPTIVE_STEP_SIZE=true
PARALLEL_THREADS=0  # 0 = use all available cores

# Output Configuration
OUTPUT_DIR=./results
LOG_LEVEL=info
SAVE_TRAJECTORIES=true
SAVE_ENERGY_PROFILES=true
SAVE_ENTROPY_DISTRIBUTIONS=true

# Performance Tuning
ENABLE_PROFILING=false
BENCHMARK_MODE=false
MEMORY_LIMIT_GB=8
CPU_AFFINITY=

# External Services
AUTOBAHN_ENDPOINT=http://localhost:9000/api/v1
AUTOBAHN_API_KEY=your_api_key_here
ENABLE_AUTOBAHN_INTEGRATION=false

# Security
JWT_SECRET=your_jwt_secret_here
API_RATE_LIMIT=1000
ENABLE_CORS=true
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# Development
DEBUG_MODE=false
ENABLE_HOT_RELOAD=false
MOCK_QUANTUM_HARDWARE=true
ENABLE_EXPERIMENTAL_FEATURES=false 